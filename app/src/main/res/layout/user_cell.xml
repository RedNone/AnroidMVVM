<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/textView"
        android:layout_width="125dp"
        android:layout_height="30dp"
        android:layout_marginBottom="@dimen/spacing_1x"
        android:layout_marginEnd="@dimen/spacing_2x"
        android:layout_marginStart="@dimen/spacing_1x"
        android:layout_marginTop="@dimen/spacing_2x"
        android:gravity="left|center_vertical"
        android:text="@string/user_label_key_name"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textSize="@dimen/text_size"
        app:layout_constraintBottom_toTopOf="@+id/textView3"
        app:layout_constraintEnd_toStartOf="@+id/textViewNameContainer"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textViewNameContainer"
        android:layout_width="210dp"
        android:layout_height="30dp"
        android:layout_marginBottom="@dimen/spacing_1x"
        android:layout_marginEnd="@dimen/spacing_2x"
        android:layout_marginStart="16dp"
        android:layout_marginTop="@dimen/spacing_2x"
        android:gravity="left|center_vertical"
        android:textSize="@dimen/text_size"
        app:layout_constraintBottom_toTopOf="@+id/textViewEmailContainer"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="125dp"
        android:layout_height="30dp"
        android:layout_marginBottom="@dimen/spacing_1x"
        android:layout_marginEnd="@dimen/spacing_1x"
        android:layout_marginStart="@dimen/spacing_1x"
        android:layout_marginTop="@dimen/spacing_1x"
        android:gravity="left|center_vertical"
        android:text="@string/user_label_key_email"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textSize="@dimen/text_size"
        app:layout_constraintBottom_toTopOf="@+id/textView5"
        app:layout_constraintEnd_toStartOf="@+id/textViewEmailContainer"
        app:layout_constraintHorizontal_bias="0.47"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/textViewEmailContainer"
        android:layout_width="210dp"
        android:layout_height="30dp"
        android:layout_marginBottom="@dimen/spacing_1x"
        android:layout_marginEnd="@dimen/spacing_2x"
        android:layout_marginStart="17dp"
        android:layout_marginTop="@dimen/spacing_1x"
        android:gravity="left|center_vertical"
        android:textSize="@dimen/text_size"
        app:layout_constraintBottom_toTopOf="@+id/textViewPhoneContainer"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/textViewNameContainer" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="124dp"
        android:layout_height="30dp"
        android:layout_marginBottom="@dimen/spacing_1x"
        android:layout_marginEnd="@dimen/spacing_1x"
        android:layout_marginStart="@dimen/spacing_1x"
        android:layout_marginTop="@dimen/spacing_1x"
        android:gravity="left|center_vertical"
        android:text="@string/user_label_key_phone"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textSize="@dimen/text_size"
        app:layout_constraintBottom_toTopOf="@+id/textView7"
        app:layout_constraintEnd_toStartOf="@+id/textViewPhoneContainer"
        app:layout_constraintHorizontal_bias="0.444"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textViewPhoneContainer"
        android:layout_width="210dp"
        android:layout_height="30dp"
        android:layout_marginBottom="@dimen/spacing_1x"
        android:layout_marginEnd="@dimen/spacing_2x"
        android:layout_marginStart="18dp"
        android:layout_marginTop="@dimen/spacing_1x"
        android:gravity="left|center_vertical"
        android:textSize="@dimen/text_size"
        app:layout_constraintBottom_toTopOf="@+id/textViewWebsiteContainer"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/textViewEmailContainer"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="124dp"
        android:layout_height="30dp"
        android:layout_marginBottom="@dimen/spacing_1x"
        android:layout_marginEnd="@dimen/spacing_1x"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:gravity="left|center_vertical"
        android:text="@string/user_label_key_website"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textSize="@dimen/text_size"
        app:layout_constraintBottom_toTopOf="@+id/view"
        app:layout_constraintEnd_toStartOf="@+id/textViewWebsiteContainer"
        app:layout_constraintHorizontal_bias="0.47"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/textViewWebsiteContainer"
        android:layout_width="211dp"
        android:layout_height="30dp"
        android:layout_marginBottom="@dimen/spacing_1x"
        android:layout_marginEnd="@dimen/spacing_2x"
        android:layout_marginStart="17dp"
        android:layout_marginTop="@dimen/spacing_1x"
        android:gravity="left|center_vertical"
        android:textSize="@dimen/text_size"
        app:layout_constraintBottom_toTopOf="@+id/view"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/textViewPhoneContainer" />

    <View
        android:id="@+id/view"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:background="@color/separatorColor"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</android.support.constraint.ConstraintLayout>